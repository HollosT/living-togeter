{"ast":null,"code":"import { computed } from '@vue/runtime-core';\nimport { useStore } from 'vuex';\nexport default {\n  setup: function setup() {\n    var store = useStore();\n    var isLoggedIn = computed(function () {\n      return store.getters.isAuthenticated;\n    });\n    function logout() {\n      store.dispatch('logout');\n    }\n    return {\n      isLoggedIn: isLoggedIn,\n      logout: logout\n    };\n  }\n};","map":{"version":3,"mappings":"AAkBA,SAASA,QAAO,QAAS,mBAAkB;AAC3C,SAASC,QAAO,QAAS,MAAK;AAE9B,eAAe;EAEbC,KAAK,mBAAG;IACN,IAAMC,KAAI,GAAIF,QAAQ,EAAC;IAEvB,IAAMG,UAAS,GAAIJ,QAAQ,CAAC,YAAM;MAChC,OAAOG,KAAK,CAACE,OAAO,CAACC,eAAc;IACrC,CAAC;IAGD,SAASC,MAAM,GAAG;MAChBJ,KAAK,CAACK,QAAQ,CAAC,QAAQ;IACzB;IAEA,OAAM;MAAEJ,UAAU,EAAVA,UAAU;MAAEG,MAAM,EAANA;IAAM;EAC5B;AAEF","names":["computed","useStore","setup","store","isLoggedIn","getters","isAuthenticated","logout","dispatch"],"sourceRoot":"","sources":["/Users/tamashollos/Git/living-togeter/src/components/layouts/TheHeader.vue"],"sourcesContent":["<template>\n  <header>\n    <nav>\n\n        <router-link to=\"/\"><h1>Living together</h1></router-link> \n\n\n        <base-button v-if=\"isLoggedIn\" @click=\"logout\" type=\"filled\">Logout</base-button> \n        <base-button v-if=\"isLoggedIn\" @click=\"logout\"  type=\"filled\">Logout</base-button> \n\n        <router-link  v-else to=\"/auth\"><h1>Login</h1></router-link> \n  \n    </nav>\n\n  </header>\n</template>\n\n<script>\nimport { computed } from '@vue/runtime-core'\nimport { useStore } from 'vuex'\n\nexport default {\n\n  setup() {\n    const store = useStore()\n\n    const isLoggedIn = computed(() => {\n      return store.getters.isAuthenticated\n    })\n\n\n    function logout() {\n      store.dispatch('logout')\n    }\n\n    return{ isLoggedIn, logout}\n  }\n\n}\n</script>\n\n<style scoped>\nheader {\n  padding: 0;\n  margin: 0;\n  box-shadow: 0 2px 2px rgba(0, 0, 0, .25);\n  background-color: #f3f3f3;\n  width: 100%;\n  \n}\nnav {\n  padding: 2vw 0